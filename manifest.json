{
   "action": {
      "default_icon": "icons/icon.png",
      "default_title": "SecureView"
   },
   "background": {
      "service_worker": "background/background.js"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "content/content-script.js" ],
      "match_origin_as_fallback": true,
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self';"
   },
   "description": "This extension captures the history of browsed url by user. It stores data securely on local machine instead of cloud.",
   "host_permissions": [ "\u003Call_urls>" ],
   "icons": {
      "128": "icons/icon.png"
   },
   "manifest_version": 3,
   "name": "SecureView",
   "permissions": [ "management", "storage", "webNavigation", "webRequest" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.0.1"
}
